        <type>residual</type>
        <concentrations unit="ng/ml">
          <min>
            <value>1</value>
            <formula/>
          </min>
          <max>
            <value>10</value>
            <formula/>
          </max>
          <best>
            <value>2.5</value>
            <formula/>
